msgstr "ЛІСОРУБ"

msgid "LIGHTER"
msgstr "ЗАПАЛЬНИЧКА"

msgid "CHORES"
msgstr "ВПРАВИ"

msgid "MIGHT"
msgstr "МОГУТНІСТЬ"

msgid "HEADLINER"
msgstr "ГЕДЛАЙНЕРКА"

msgid "SURVIVAL"
msgstr "ВИЖИВАННЯ"

msgid "MERMDOM"
msgstr "МЕРМІВСТВО"

msgid "TORCH"
msgstr "СМОЛОСКИП"

